<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <configSections>
    <sectionGroup name="SAP.Middleware.Connector">
      <sectionGroup name="ClientSettings">
        <section name="DestinationConfiguration" type="SAP.Middleware.Connector.RfcDestinationConfigurat ion, sapnco"/>
      </sectionGroup>
    </sectionGroup>
  </configSections>
  <SAP.Middleware.Connector>
    <ClientSettings>
      <DestinationConfiguration>
        <destinations>
          <add NAME="NCO_TESTS" USER="CHANDRAS" PASSWD="init123" CLIENT="100" Cache-Control="no-cache" Content-Type="application/json" LANG="EN" ASHOST="sapbrillio.cloudapp.net" SYSNR="10" POOL_SIZE="5" MAX_POOL_SIZE="10"/>
        </destinations>
      </DestinationConfiguration>
    </ClientSettings>
  </SAP.Middleware.Connector>
  <appSettings>
    <add key="NAME" value="E6D"/>
    <add key="SAP_APPSERVERHOST" value="sapbrillio.cloudapp.net"/>
    <add key="SAP_USERNAME" value="CHANDRAS"/>
    <add key="SAP_PASSWORD" value="init123"/>
    <add key="SAP_CLIENT" value="100"/>
    <add key="SAP_SYSTEMNUM" value="0"/>
    <add key="SAP_LANGUAGE" value="EN"/>
    <add key="SAP_POOLSIZE" value="10"/>
  </appSettings>


</configuration>
